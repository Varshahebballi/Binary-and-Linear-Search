#include <iostream>
using namespace std;

int countOccurrences(int arr[], int n, int target) {
    int count = 0;
    for (int i = 0; i < n; i++) {
        if (arr[i] == target) count++;
    }
    return count;
}

int main() {
    int arr[] = {1, 2, 2, 3, 2, 5};
    int target = 2;
    cout << "Count of Occurrences: " << countOccurrences(arr, 6, target) << endl;
    return 0;
}
